<div class='jumbotron'>
  <h1><%= @greetings[:welcome] %></h1>
  <div class="row">
    <% if current_user %>
      <%= button_to 'Log out', destroy_user_session_path, method: :delete %>
      <%= button_to 'Travel notes', travel_notes_path, method: :get %>
    <% else %>
      <%= button_to 'Login with google!', user_google_oauth2_omniauth_authorize_path, method: :get %>
    <% end %>
  </div>
</div>

